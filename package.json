{
  "name": "monday-ui",
  "version": "0.1.0",
  "description": "This is a GUI for Monday developer tool",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/eko/monday-app.git"
  },
  "homepage": "https://vincent.composieux.fr",
  "bugs": {
    "url": "https://github.com/eko/monday-app/issues"
  },
  "author": {
    "name": "Vincent Composieux",
    "email": "monday@composieux.fr",
    "url": "https://github.com/eko"
  },
  "keywords": [
    "monday",
    "local",
    "apps",
    "forward",
    "kubernetes",
    "ssh",
    "open-source"
  ],
  "engines": {
    "node": ">=9.0.0"
  },
  "main": "main.js",
  "scripts": {
    "prod": "webpack --mode production --config webpack.build.config.js && electron --noDevServer .",
    "start": "webpack-dev-server --hot --host 0.0.0.0 --config=./webpack.dev.config.js --mode development",
    "build": "webpack --config webpack.build.config.js --mode production",
    "package": "npm run build",
    "postpackage": "electron-packager ./ --out=./builds"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.1",
    "electron": "^6.0.0",
    "google-protobuf": "^3.9.0",
    "grpc": "^1.22.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "sudo-prompt": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "babel-loader": "^8.0.6",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^3.2.0",
    "electron-packager": "^14.0.3",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.1.2",
    "postcss-pxtorem": "^4.0.1",
    "style-loader": "^1.0.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  }
}
